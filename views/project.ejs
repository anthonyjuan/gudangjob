            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2><%= project.name %></h2>
                    <p class="lead"><%= project.description %></a>.</p>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
